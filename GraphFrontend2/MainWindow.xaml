<Window x:Class="GraphFrontend2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphFrontend2"
        mc:Ignorable="d"
        Title="MainWindow" Height="900" Width="1800">
    <Grid>
        <Canvas x:Name="canvas1" Margin="50,50,50,50" Background="#FFD8E0E6" OpacityMask="#FFD6E2E2" MouseLeftButtonDown="CanvasButtonClick"/>
        <Button x:Name="SettingsButton" Content="Settings" HorizontalAlignment="Left" Margin="1680,30,0,0" VerticalAlignment="Top" Click="SettingsButtonClick" Width="70"/>
        <Button x:Name="LoadSavedButton" Content="Load" HorizontalAlignment="Left" Margin="1600,30,0,0" VerticalAlignment="Top" Click="LoadButtonClick" Width="70"/>
        <Button x:Name="SaveButton" Content="Save" HorizontalAlignment="Left" Margin="1520,30,0,0" VerticalAlignment="Top" Click="SaveButtonClick" Width="70"/>
        <Button x:Name="NewButton" Content="New" HorizontalAlignment="Left" Margin="1440,30,0,0" VerticalAlignment="Top" Click="NewButtonClick" Width="70"/>
        <TextBox x:Name="CommandTB" HorizontalAlignment="Left" Margin="50,30,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="800" KeyDown="tbkeydown"/>
    </Grid>
</Window>
